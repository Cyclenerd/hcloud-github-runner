{
  "name": $name,
  "location": $location,
  "server_type": $server_type,
  "start_after_create": true,
  "image": $image,
  "ssh_keys": [],
  "volumes": [],
  "networks": [],
  "user_data": $cloud_init_yml,
  "labels": {
    "type": "github-runner",
    "os-image": $image,
    "gh-ver": $runner_version,
    "gh-owner-id": $github_owner_id,
    "gh-repo-id": $github_repo_id
  },
  "automount": true,
  "public_net": {
    "enable_ipv4": $enable_ipv4,
    "enable_ipv6": $enable_ipv6,
  }
}
